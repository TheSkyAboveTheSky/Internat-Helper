pipeline {
  agent any
  
  stages {
  
    
    stage('Build') {
      steps {
        bat 'cd server'
        bat 'mvn clean package'
      }
    }
    
    stage('Test') {
      steps {
        bat 'cd server'
        bat 'mvn test'
      }
    }
    
    stage('Deploy') {
      steps {
        bat 'cd server'
        bat 'mvn deploy'
      }
    }
  }
}
